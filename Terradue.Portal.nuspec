    <file src="Terradue.Portal.AdminTool/bin/Terradue.Portal.AdminTool.exe" target="lib/net40/Terradue.Portal.AdminTool.exe" />
    <file src="Terradue.Portal.Agent/bin/Terradue.Portal.Agent.exe" target="lib/net40/Terradue.Portal.Agent.exe" />
