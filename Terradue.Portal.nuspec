<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Terradue.Portal</id>
    <version>2.6.3</version>
    <title>Terradue.Portal</title>
    <authors>Terradue</authors>
    <!--<projectUrl>https://github.com/Terradue/DotNetOpenSearch</projectUrl>
